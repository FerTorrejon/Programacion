#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main(){
	
	int num[3][3];
	for (int i=0;i<3;i++){
		for(int j=0;j<3;j++){
			cout<<"Digite un numero ["<<i<<"]["<<j<<"]";
			cin>>num[i][j];
		}
	}
	cout<<"Matriz Original\n";
	for(int i=0;i<3;i++){
		for( int j=0;j<3;j++){
			cout<<num[i][j]<<" ";
		}
		cout<<"\n";
	}
	cout<<"\nMatriz Transpuesta\n";
	for(int i=0;i<3;i++){
		for( int j=0;j<3;j++){
			cout<<num[j][i]<<" ";
		}
		cout<<"\n";
	}
getch();
}	
