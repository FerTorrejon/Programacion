// Fibonacci.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{ 
 int A,B,C,i,n;
  A=1;
  B=1;
  cout<<"Ingrese el numero de veces: ";
  cin>>n;
   i=1;
   while (i<=n-2)
    {
	 C=A+B;
	 A=B;
	 B=C;
	 i=i+1;
    }
   cout<<"EL resultado es: "<<C<<endl;
   getch();
}


====================PSeInt==========================
Algoritmo Fibonacci
	A <- 1
	B <- 1
	Escribir "Ingrese N: ";
	Leer N;
	Escribir A;
	Escribir B;
	i <- 1;
	Mientras i<=N-2 Hacer
		C <- A+B;
		A <- B;
		B <- C;
		Escribir C;
		i <- i+1;
	FinMientras
	Escribir "El resultado es: ",C;
FinAlgoritmo
